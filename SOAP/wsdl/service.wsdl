<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:xs="http://www.w3.org/2001/XMLSchema"
             xmlns:tns="http://example.com/zoo_soap"
             targetNamespace="http://example.com/zoo_soap"
             name="ZooService">

    <!-- Tipos de datos -->
    <types>
        <xs:schema targetNamespace="http://example.com/zoo_soap">

            <!-- Operaciones relacionadas con zoológicos -->
            <xs:element name="getZooInfoRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="getZooInfoResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="name" type="xs:string"/>
                        <xs:element name="city" type="xs:string"/>
                        <xs:element name="foundation" type="xs:string"/>
                        <xs:element name="location" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="addZooRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                        <xs:element name="name" type="xs:string"/>
                        <xs:element name="city" type="xs:string"/>
                        <xs:element name="foundation" type="xs:string"/>
                        <xs:element name="location" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="addZooResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="deleteZooRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="deleteZooResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="updateZooInfoRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                        <xs:element name="name" type="xs:string" minOccurs="0"/>
                        <xs:element name="city" type="xs:string" minOccurs="0"/>
                        <xs:element name="foundation" type="xs:string" minOccurs="0"/>
                        <xs:element name="location" type="xs:string" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="updateZooInfoResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="listZoosRequest">
                <xs:complexType/>
            </xs:element>
            <xs:element name="listZoosResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoos" type="xs:string" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="listZoosWithAnimalsRequest">
                <xs:complexType/>
            </xs:element>
            <xs:element name="listZoosWithAnimalsResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoos" type="xs:string" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="getZooWithMostAnimalsRequest">
                <xs:complexType/>
            </xs:element>
            <xs:element name="getZooWithMostAnimalsResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                        <xs:element name="name" type="xs:string"/>
                        <xs:element name="animal_count" type="xs:integer"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <!-- Operaciones relacionadas con animales -->
            <xs:element name="getAnimalRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animal_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="getAnimalResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="name" type="xs:string"/>
                        <xs:element name="species" type="xs:string"/>
                        <xs:element name="scientific_name" type="xs:string"/>
                        <xs:element name="habitat" type="xs:string"/>
                        <xs:element name="diet" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="addAnimalRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animal_id" type="xs:string"/>
                        <xs:element name="zoo_id" type="xs:string"/>
                        <xs:element name="name" type="xs:string"/>
                        <xs:element name="species" type="xs:string"/>
                        <xs:element name="scientific_name" type="xs:string"/>
                        <xs:element name="habitat" type="xs:string"/>
                        <xs:element name="diet" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="addAnimalResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="deleteAnimalRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animal_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="deleteAnimalResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="updateAnimalInfoRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animal_id" type="xs:string"/>
                        <xs:element name="name" type="xs:string" minOccurs="0"/>
                        <xs:element name="species" type="xs:string" minOccurs="0"/>
                        <xs:element name="scientific_name" type="xs:string" minOccurs="0"/>
                        <xs:element name="habitat" type="xs:string" minOccurs="0"/>
                        <xs:element name="diet" type="xs:string" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="updateAnimalInfoResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="message" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="listAnimalsRequest">
                <xs:complexType/>
            </xs:element>
            <xs:element name="listAnimalsResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animals" type="xs:string" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="getAnimalsByZooRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="getAnimalsByZooResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animals" type="xs:string" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>


            <xs:element name="countAnimalsInZooRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="countAnimalsInZooResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="zoo_id" type="xs:string"/>
                        <xs:element name="animal_count" type="xs:integer"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="getAnimalsBySpeciesRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="species" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="getAnimalsBySpeciesResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="animals" type="xs:string" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:schema>
    </types>

    <!-- Mensajes -->
    <message name="getZooInfoRequest">
        <part name="parameters" element="tns:getZooInfoRequest"/>
    </message>
    <message name="getZooInfoResponse">
        <part name="parameters" element="tns:getZooInfoResponse"/>
    </message>

    <message name="addZooRequest">
        <part name="parameters" element="tns:addZooRequest"/>
    </message>
    <message name="addZooResponse">
        <part name="parameters" element="tns:addZooResponse"/>
    </message>

    <message name="deleteZooRequest">
        <part name="parameters" element="tns:deleteZooRequest"/>
    </message>
    <message name="deleteZooResponse">
        <part name="parameters" element="tns:deleteZooResponse"/>
    </message>

    <message name="updateZooInfoRequest">
        <part name="parameters" element="tns:updateZooInfoRequest"/>
    </message>
    <message name="updateZooInfoResponse">
        <part name="parameters" element="tns:updateZooInfoResponse"/>
    </message>

    <message name="listZoosRequest">
        <part name="parameters" element="tns:listZoosRequest"/>
    </message>
    <message name="listZoosResponse">
        <part name="parameters" element="tns:listZoosResponse"/>
    </message>

    <message name="listZoosWithAnimalsRequest">
        <part name="parameters" element="tns:listZoosWithAnimalsRequest"/>
    </message>
    <message name="listZoosWithAnimalsResponse">
        <part name="parameters" element="tns:listZoosWithAnimalsResponse"/>
    </message>

    <message name="getZooWithMostAnimalsRequest">
        <part name="parameters" element="tns:getZooWithMostAnimalsRequest"/>
    </message>
    <message name="getZooWithMostAnimalsResponse">
        <part name="parameters" element="tns:getZooWithMostAnimalsResponse"/>
    </message>

    <message name="getAnimalRequest">
        <part name="parameters" element="tns:getAnimalRequest"/>
    </message>
    <message name="getAnimalResponse">
        <part name="parameters" element="tns:getAnimalResponse"/>
    </message>

    <message name="addAnimalRequest">
        <part name="parameters" element="tns:addAnimalRequest"/>
    </message>
    <message name="addAnimalResponse">
        <part name="parameters" element="tns:addAnimalResponse"/>
    </message>

    <message name="deleteAnimalRequest">
        <part name="parameters" element="tns:deleteAnimalRequest"/>
    </message>
    <message name="deleteAnimalResponse">
        <part name="parameters" element="tns:deleteAnimalResponse"/>
    </message>

    <message name="updateAnimalInfoRequest">
        <part name="parameters" element="tns:updateAnimalInfoRequest"/>
    </message>
    <message name="updateAnimalInfoResponse">
        <part name="parameters" element="tns:updateAnimalInfoResponse"/>
    </message>

    <message name="listAnimalsRequest">
        <part name="parameters" element="tns:listAnimalsRequest"/>
    </message>
    <message name="listAnimalsResponse">
        <part name="parameters" element="tns:listAnimalsResponse"/>
    </message>

    <message name="getAnimalsByZooRequest">
        <part name="parameters" element="tns:getAnimalsByZooRequest"/>
    </message>
    <message name="getAnimalsByZooResponse">
        <part name="parameters" element="tns:getAnimalsByZooResponse"/>
    </message>

    <message name="countAnimalsInZooRequest">
        <part name="parameters" element="tns:countAnimalsInZooRequest"/>
    </message>
    <message name="countAnimalsInZooResponse">
        <part name="parameters" element="tns:countAnimalsInZooResponse"/>
    </message>

    <message name="getAnimalsBySpeciesRequest">
        <part name="parameters" element="tns:getAnimalsBySpeciesRequest"/>
    </message>
    <message name="getAnimalsBySpeciesResponse">
        <part name="parameters" element="tns:getAnimalsBySpeciesResponse"/>
    </message>

    <!-- Puerto y operación -->
    <portType name="ZooServicePortType">
        <operation name="getZooInfo">
            <documentation>Obtener información de un zoológico.</documentation>
            <input message="tns:getZooInfoRequest"/>
            <output message="tns:getZooInfoResponse"/>
        </operation>

        <operation name="addZoo">
            <documentation>Agregar un zoológico.</documentation>
            <input message="tns:addZooRequest"/>
            <output message="tns:addZooResponse"/>
        </operation>

        <operation name="deleteZoo">
            <documentation>Eliminar un zoológico.</documentation>
            <input message="tns:deleteZooRequest"/>
            <output message="tns:deleteZooResponse"/>
        </operation>

        <operation name="updateZooInfo">
            <documentation>Actualizar información de un zoológico.</documentation>
            <input message="tns:updateZooInfoRequest"/>
            <output message="tns:updateZooInfoResponse"/>
        </operation>

        <operation name="listZoos">
            <documentation>Listar todos los zoológicos.</documentation>
            <input message="tns:listZoosRequest"/>
            <output message="tns:listZoosResponse"/>
        </operation>

        <operation name="listZoosWithAnimals">
            <documentation>Listar zoológicos junto con sus animales.</documentation>
            <input message="tns:listZoosWithAnimalsRequest"/>
            <output message="tns:listZoosWithAnimalsResponse"/>
        </operation>

        <operation name="getZooWithMostAnimals">
            <documentation>Obtener el zoológico con más animales.</documentation>
            <input message="tns:getZooWithMostAnimalsRequest"/>
            <output message="tns:getZooWithMostAnimalsResponse"/>
        </operation>

        <operation name="getAnimal">
            <documentation>Obtener información de un animal.</documentation>
            <input message="tns:getAnimalRequest"/>
            <output message="tns:getAnimalResponse"/>
        </operation>

        <operation name="addAnimal">
            <documentation>Agregar un animal a un zoológico.</documentation>
            <input message="tns:addAnimalRequest"/>
            <output message="tns:addAnimalResponse"/>
        </operation>

        <operation name="deleteAnimal">
            <documentation>Eliminar un animal.</documentation>
            <input message="tns:deleteAnimalRequest"/>
            <output message="tns:deleteAnimalResponse"/>
        </operation>

        <operation name="updateAnimalInfo">
            <documentation>Actualizar información de un animal.</documentation>
            <input message="tns:updateAnimalInfoRequest"/>
            <output message="tns:updateAnimalInfoResponse"/>
        </operation>

        <operation name="listAnimals">
            <documentation>Listar todos los animales disponibles.</documentation>
            <input message="tns:listAnimalsRequest"/>
            <output message="tns:listAnimalsResponse"/>
        </operation>

        <operation name="getAnimalsByZoo">
            <documentation>Obtener todos los animales asociados a un zoológico específico.</documentation>
            <input message="tns:getAnimalsByZooRequest"/>
            <output message="tns:getAnimalsByZooResponse"/>
        </operation>

        <operation name="countAnimalsInZoo">
            <documentation>Contar animales en un zoológico.</documentation>
            <input message="tns:countAnimalsInZooRequest"/>
            <output message="tns:countAnimalsInZooResponse"/>
        </operation>

        <operation name="getAnimalsBySpecies">
            <documentation>Obtener animales por especie.</documentation>
            <input message="tns:getAnimalsBySpeciesRequest"/>
            <output message="tns:getAnimalsBySpeciesResponse"/>
        </operation>
    </portType>

    <!-- Protocolo de enlace -->
    <binding name="ZooServiceBinding" type="tns:ZooServicePortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>

        <operation name="getZooInfo">
            <soap:operation soapAction="http://example.com/zoo_soap#getZooInfo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="addZoo">
            <soap:operation soapAction="http://example.com/zoo_soap#addZoo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="deleteZoo">
            <soap:operation soapAction="http://example.com/zoo_soap#deleteZoo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="updateZooInfo">
            <soap:operation soapAction="http://example.com/zoo_soap#updateZooInfo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="listZoos">
            <soap:operation soapAction="http://example.com/zoo_soap#listZoos"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="listZoosWithAnimals">
            <soap:operation soapAction="http://example.com/zoo_soap#listZoosWithAnimals"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="getZooWithMostAnimals">
            <soap:operation soapAction="http://example.com/zoo_soap#getZooWithMostAnimals"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="getAnimal">
            <soap:operation soapAction="http://example.com/zoo_soap#getAnimal"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="addAnimal">
            <soap:operation soapAction="http://example.com/zoo_soap#addAnimal"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="deleteAnimal">
            <soap:operation soapAction="http://example.com/zoo_soap#deleteAnimal"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="updateAnimalInfo">
            <soap:operation soapAction="http://example.com/zoo_soap#updateAnimalInfo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="countAnimalsInZoo">
            <soap:operation soapAction="http://example.com/zoo_soap#countAnimalsInZoo"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>

        <operation name="getAnimalsBySpecies">
            <soap:operation soapAction="http://example.com/zoo_soap#getAnimalsBySpecies"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>

    <!-- Servicio -->
    <service name="ZooService">
        <port name="ZooServicePort" binding="tns:ZooServiceBinding">
            <soap:address location="http://localhost:8000/"/>
        </port>
    </service>
</definitions>
